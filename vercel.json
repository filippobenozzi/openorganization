{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/fonts/ibm-plex-sans-var.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/ibm-plex-sans-var-italic.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/tweets",
      "destination": "/bookmarks"
    },
    {
      "source": "/capsules",
      "destination": "/bookmarks"
    },
    {
      "source": "/blog/is-aml-threatening-the-crypto-assets-or-the-opposite",
      "destination": "/blog/are-aml-directives-threatening-crypto-assets-or-the-opposite"
    },
    {
      "source": "/blog/can-central-banks-regulate-cryptocurrencies",
      "destination": "/blog/are-aml-directives-threatening-crypto-assets-or-the-opposite"
    },
    {
      "source": "/blog/hello",
      "destination": "/blog/welcome-to-my-personal-open-source-space"
    }
  ]
}
